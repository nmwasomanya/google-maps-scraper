{
  "name": "google-maps-scraper",
  "version": "1.0.0",
  "description": "Google Maps scraper using Playwright",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "scrape": "npm run build && node dist/index.js",
    "web": "npm run build && node dist/web/server.js",
    "web:dev": "ts-node src/web/server.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0"
  }
}
